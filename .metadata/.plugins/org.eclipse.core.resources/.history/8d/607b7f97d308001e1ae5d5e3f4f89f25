package JavaPrograms;

public class RemoveSpecialCharFromStringArray {
	
public void removespecialchar(String a)
{
	
}
 public static void main(String args[])
 {
	 int i,j;
	 String[] input = {"abcd","@efg","xyz"} ;
	 for( i=0;i<=input.length-1;i++)
	 {
		 
		 for(j=0;j<input[i].length()-i;i++)
		 {
			 System.out.println("----"+input[i].length());
			 System.out.println("output"+input[i].charAt(j));
				 
			 
		 }
	 }
 }

}
