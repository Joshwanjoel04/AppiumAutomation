package JavaPrograms;

public class InterviewA {
	


			public static void removeString(String a)
			{
			 char[] ch = a.toCharArray();
			char[] newChararry ;

			 for(int i=0;i<ch.length-1;i++)
			{
			 if(ch[i]!=' ')
			{
			 newChararry+ = ch[i];
			}


			}
			String newUpdatedString= newChararry.toString();
			System.out.println("new updated string is"+newUpdatedString);



			}
			
			
	    public static void DuplicateWordsRemoval()
	    {
	    	
	    }
	
	

	public static void main(String[] args) {
		String a ="gbk lkjgl";
		removeString(a);

	}

}
